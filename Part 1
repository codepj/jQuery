    <script>
  var now = new Date();
  var ticks = now.getTime();  
  
  window.onload = function() {
    var newDate = new Date();
    var diff = newDate.getTime() - ticks;
  
    console.log("Onload Diff:" + diff);  
  }
  
  $( document ).ready(function() {
    var newDate = new Date();
    var diff = newDate.getTime() - ticks;
  
    console.log("Ready Diff:" + diff);  
    
    $('#marsLink').click(function( event ){
      // event.preventDefault();
      
      var x = "";
      var r=confirm("Press a button");
      if (r==true)
      {
        return true;
      }
      else
      {
        return false;
      } 
      
      console.log(x);
    });
  });
  </script>
